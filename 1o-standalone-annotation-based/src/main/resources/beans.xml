<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.0.xsd">

<!-- Required to register all the annotation config related beans  -->
<context:annotation-config></context:annotation-config>

<bean class="com.skbr.Car"></bean>
<bean name="v8" class="com.skbr.Engine"></bean>
<bean name="sony" class="com.skbr.AudioSystem"></bean>

<bean class="com.skbr.Mobile"></bean>
<bean name="exide" class="com.skbr.Battery"></bean>


</beans>
